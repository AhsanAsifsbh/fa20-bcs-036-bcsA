


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" 
    integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">





    </head>
  <body>
  
     
     

    <div class="container">
      <div class="row">
        <% for (let index = 0; index < students.length; index++) { %>
          <div class="col-md-3">
            <div class="card">
              <img class="card-img-top" src="/images/<%=students[index].productImage%>" alt="Product 1">
              <div class="card-body">
                <h4 class="card-title"><%=students[index].productName%></h4>
               
                <div class="d-flex justify-content-between align-items-center">
                  <div class="btn-group" role="group" aria-label="Product Actions">
                    <a href="/edit:<%=students[index]._id%>" type="button" class="btn btn-primary btn-sm">Edit</a>
                    <a href="/delete:<%=students[index]._id%>" type="button" class="btn btn-outline-dark btn-sm">
                      <i class="fas fa-info-circle me-2"></i>Delete
                    </a>
                  </div>
                  



                </div>
              </div>
            </div>
          </div>
        <% } %>
      </div>
    </div>
    

         










         








          

</body>
</html>  





